  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "yarn build-storybook && next build",
    "start": "next start",
    "start:dev": "yarn build-storybook && next build && next start -p 3001",
    "lint": "next lint --dir ./src",
    "test": "jest",
    "test-dev": "jest --watchAll",
    "test-dev-cov": "jest --watchAll --coverage",
    "test-ci": "jest --ci --coverage",
    "storybook": "start-storybook -p 6006 -c .storybook/dev",
    "build-storybook": "build-storybook --quiet -c .storybook/prod && rm -rf ./public/storybook && mv ./storybook-static ./public/storybook",
    "api:sync": "generateTypesFromUrl NEXT_PUBLIC_API_URL -json ./src/api/main-backend/specs && eslint ./src/api/main-backend/specs --fix"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@gogaille/storybook-addon-next-router": "^4.0.0",
