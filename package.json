  "scripts": {
    "dev": "next dev",
    "build": "pnpm build-storybook && next build",
    "start": "next start",
    "start:dev": "pnpm build-storybook && next build && next start -p 3001",
    "lint": "next lint --dir ./src",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test-dev": "jest --watchAll",
    "test-dev-cov": "jest --watchAll --coverage",
    "test-ci": "jest --ci --coverage",
    "storybook": "start-storybook -p 6006 -c .storybook/dev",
    "build-storybook": "build-storybook --quiet -c .storybook/prod && rm -rf ./public/storybook && mv ./storybook-static ./public/storybook",
    "api:sync": "generateTypesFromUrl -u https://rhf-mui-nx-sandbox-back.herokuapp.com/-json -o ./src/api/main-backend/specs",
    "sync-icons": "pnpm generateReadmeIcons -h 70"
  },
